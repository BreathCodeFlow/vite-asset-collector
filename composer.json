{
	"name": "praetorius/vite-asset-collector",
	"description": "Use AssetCollector to embed assets generated by vite",
	"type": "typo3-cms-extension",
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"typo3/cms-core": "^12.4"
	},
	"require-dev": {
		"typo3/coding-standards": "^0.7.1",
		"editorconfig-checker/editorconfig-checker": "^10.3"
	},
	"autoload": {
		"psr-4": {
			"Praetorius\\ViteAssetCollector\\": "Classes/"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"extra": {
		"typo3/cms": {
			"web-dir": ".Build/Web",
			"extension-key": "vite_asset_collector"
		}
	},
	"scripts": {
		"lint": [
			"php-cs-fixer fix --diff --dry-run",
			"ec ."
		]
	}
}
